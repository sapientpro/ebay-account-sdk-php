{
    "name": "sapientpro/ebay-account-sdk-php",
    "description": "eBay Account API",
    "keywords": [
        "ebay",
        "account",
        "sdk",
        "api"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/serializer": "^6.2",
        "symfony/property-access": "^6.2",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": { "SapientPro\\EbayAccountSDK\\" : "src/"}
    },
    "autoload-dev": {
        "psr-4": { "SapientPro\\EbayAccountSDK\\Tests\\" : "tests/" }
    }
}
